<?xml version="1.0" encoding="utf-8"?>
<includes>
	<default type="label">
		<label />
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>white2</textcolor>
		<disabledcolor>disabled</disabledcolor>
		<selectedcolor>$VAR[HighlightColorVar]</selectedcolor>
		<scrollsuffix> â€¢ </scrollsuffix>
		<scrollspeed>40</scrollspeed>
		<scroll>false</scroll>
	</default>
	<default type="fadelabel">
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>white2</textcolor>
		<selectedcolor>$VAR[HighlightColorVar]</selectedcolor>
		<scrollspeed>40</scrollspeed>
		<pauseatend>5000</pauseatend>
		<scrollout>false</scrollout>
	</default>
	<default type="edit">
		<textoffsetx>10</textoffsetx>
		<aligny>center</aligny>
		<texturefocus />
		<texturenofocus />
		<font>Font30_Reg</font>
		<textcolor>white2</textcolor>
		<focusedcolor>$VAR[HighlightColorVar]</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
		<invalidcolor>invalid</invalidcolor>
	</default>
	<default type="textbox">
		<align>justify</align>
		<font>Font30_Reg</font>
		<textcolor>white2</textcolor>
		<autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
	</default>
	<default type="grouplist">
		<itemgap>0</itemgap>
	</default>
	<default type="image">
		<aspectratio>stretch</aspectratio>
		<texture />
	</default>
	<default type="multiimage">
		<fadetime>TextureFadeTime</fadetime>
		<timeperimage>7000</timeperimage>
	</default>
	<default type="button">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
		<invalidcolor>invalid</invalidcolor>
	</default>
	<default type="radiobutton">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<textureradioon>buttons/radiobutton-on.png</textureradioon>
		<textureradiooff>buttons/radiobutton-off.png</textureradiooff>
		<radiowidth>62</radiowidth>
		<radioheight>35</radioheight>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="togglebutton">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="spincontrol">
		<textureup>buttons/spin-up-nofocus.png</textureup>
		<texturedown>buttons/spin-down-nofocus.png</texturedown>
		<textureupfocus>buttons/spin-up-focus.png</textureupfocus>
		<texturedownfocus>buttons/spin-down-focus.png</texturedownfocus>
		<textureupdisabled colordiffuse="disabled">buttons/spin-up-nofocus.png</textureupdisabled>
		<texturedowndisabled colordiffuse="disabled">buttons/spin-down-nofocus.png</texturedowndisabled>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="spincontrolex">
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<textureup>buttons/spin-up-nofocus.png</textureup>
		<texturedown>buttons/spin-down-nofocus.png</texturedown>
		<textureupfocus>buttons/spin-up-focus.png</textureupfocus>
		<texturedownfocus>buttons/spin-down-focus.png</texturedownfocus>
		<spinwidth>32</spinwidth>
		<spinheight>35</spinheight>
		<textoffsetx>10</textoffsetx>
		<font>Font30_Reg</font>
		<aligny>center</aligny>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
	<default type="scrollbar">
		<width>38</width>
		<texturesliderbackground />
		<texturesliderbar />
		<texturesliderbarfocus />
		<textureslidernib>buttons/scrollbar-nib-nofocus.png</textureslidernib>
		<textureslidernibfocus colordiffuse="$VAR[HighlightColorVar]">buttons/scrollbar-nib-focus.png</textureslidernibfocus>
		<showonepage>false</showonepage>
		<orientation>vertical</orientation>
	</default>
	<default type="progress">
		<height>20</height>
		<reveal>true</reveal>
		<texturebg colordiffuse="$VAR[PanelColorVar]" border="8">dialogs/progress-control/progress-back.png</texturebg>
		<lefttexture />
		<midtexture colordiffuse="$VAR[HighlightColorVar]" border="8">dialogs/progress-control/progress-mid.png</midtexture>
		<righttexture />
		<overlaytexture />
	</default>
	<default type="slider">
		<texturesliderbar border="15">buttons/slider-back.png</texturesliderbar>
		<textureslidernib>buttons/slider-nofocus.png</textureslidernib>
		<textureslidernibfocus>buttons/slider-focus.png</textureslidernibfocus>
		<orientation>horizontal</orientation>
		<width>200</width>
		<height>34</height>
	</default>
	<default type="sliderex">
		<texturesliderbar border="15">buttons/slider-back.png</texturesliderbar>
		<textureslidernib>buttons/slider-nofocus.png</textureslidernib>
		<textureslidernibfocus>buttons/slider-focus.png</textureslidernibfocus>
		<texturefocus border="10" colordiffuse="$VAR[HighlightColorVar]">buttons/list-select-focus.png</texturefocus>
		<texturenofocus border="30" colordiffuse="$VAR[PanelColorVar]">buttons/list-select-nofocus.png</texturenofocus>
		<sliderwidth>200</sliderwidth>
		<sliderheight>34</sliderheight>
		<aligny>center</aligny>
		<font>Font30_Reg</font>
		<textcolor>grey</textcolor>
		<focusedcolor>white</focusedcolor>
		<disabledcolor>disabled</disabledcolor>
	</default>
</includes>